    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: QuantumKat Debug",
            "type": "coreclr",
            "env": {
                "TOKEN_TYPE": "${input:TOKEN_TYPE}"
            },
            "logging": {"moduleLoad": false},
            "request": "launch",
            "preLaunchTask": "dotnet: build c:/Users/nickl/code/C#/QuantumKatSharp/QuantumKat/QuantumKat.csproj",
            "program": "${workspaceFolder}/QuantumKat/bin/Debug/net8.0/QuantumKat.dll"
        }
    ],
    "inputs": [
        {
            "type": "pickString",
            "id": "TOKEN_TYPE",
            "description": "Select the bot mode",
            "options": [
                "main",
                "dev"
            ],
            "default": "main",
        }
    ]
}